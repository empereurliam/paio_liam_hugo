#!/bin/bash

# at root directory:
rm *.log

#hackagames:
cd hackagames
if [ -d "build" ];then
rm -fr build
fi

if [ -d "__pycache__" ];then
rm -fr __pycache__
fi

#risky:
cd ../game-risky

if [ -d "build" ];then
rm -fr build
fi
if [ -d "__pycache__" ];then
rm -fr __pycache__
fi

#421:
cd ../game-421

if [ -d "build" ];then
rm -fr build
fi
if [ -d "__pycache__" ];then
rm -fr __pycache__
fi
